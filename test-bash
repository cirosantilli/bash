# Used to test how bash works, and to see if this folder is
# bash PATH.
#
#------------------------------------------------------------

# immediate:
echo -e '\ntest-bash'

echo -e '\necho $1:'
echo $1

echo -e '\necho $2:'
echo $2

echo -e '\necho $#:'
echo $#

echo -e '\necho $@:'
echo $@

echo -e '\necho $*'
echo $*

echo -e '\necho test-bash2 "$1" "$2"'
test-bash2 "$1" "$2"

echo -e '\necho test-bash2 $*'
test-bash2 $*

echo -e '\necho test-bash2 "$*"'
test-bash2 "$*"

echo -e '\necho test-bash2 $@'
test-bash2 $@

echo -e '\necho test-bash2 $@'
test-bash2 "$@"
